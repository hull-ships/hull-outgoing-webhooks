{
  "name": "hull-outgoing-webhooks",
  "version": "0.3.0",
  "description": "Send data to webhooks",
  "homepage": "https://github.com/hull-ships/hull-outgoing-webhooks",
  "repository": {
    "type": "git",
    "url": "https://github.com/hull-ships/hull-outgoing-webhooks"
  },
  "bugs": {
    "url": "https://github.com/hull-ships/hull-outgoing-webhooks/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Hull",
    "email": "contact@hull.io",
    "url": "https://hull.io"
  },
  "keywords": [
    "hull",
    "ship",
    "connector",
    "hull-ship",
    "hull-connector",
    "hull-connector-template-v0.1.0"
  ],
  "engines": {
    "node": "8.11.x",
    "npm": "5.6.x",
    "yarn": "1.6.x"
  },
  "lint-staged": {
    "*.{js,jsx,css,scss,json}": [
      "prettier --write --single-quote",
      "git add"
    ]
  },
  "scripts": {
    "ngrok": "ngrok http 8082 --region eu --subdomain hull-outgoing-webhooks",
    "build": "yarn run build:clean && yarn run build:server && yarn run build:client",
    "build:clean": "rimraf dist && rimraf build",
    "build:server": "babel server -d build",
    "build:client": "NODE_ENV=production webpack --config ./webpack.config.js",
    "start": "node -r newrelic build",
    "start:dev": "NODE_ENV=development LOG_LEVEL=debug babel-watch -L server",
    "dev": "yarn start:dev",
    "test": "yarn run test:lint && yarn run test:combined",
    "test:lint": "eslint server src",
    "test:flow": "flow check",
    "test:combined": "NODE_ENV=test jest ./test",
    "test:unit": "NODE_ENV=test jest ./test/unit",
    "test:integration": "NODE_ENV=test jest ./test/integration",
    "postinstall": "yarn run build"
  },
  "dependencies": {
    "autoprefixer": "^8.6.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "builder": "^3.2.3",
    "classnames": "^2.2.5",
    "css": "^2.2.1",
    "css-loader": "^0.28.11",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "happypack": "^5.0.0",
    "hull": "0.13.15",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "newrelic": "^4.1.3",
    "postcss-flexbugs-fixes": "^3.3.1",
    "postcss-loader": "^2.1.5",
    "progress-bar-webpack-plugin": "^1.11.0",
    "react": "^16.4.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.2",
    "style-loader": "^0.21.0",
    "superagent": "^3.8.2",
    "superagent-throttle": "^0.2.5",
    "webpack": "3.x.x",
    "webpack-dev-middleware": "2.x.x",
    "webpack-hot-middleware": "^2.22.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "babel-register": "^6.26.0",
    "babel-watch": "^2.0.7",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-flowtype-errors": "^3.6.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.9.1",
    "flow-bin": "^0.73.0",
    "flow-typed": "^2.4.0",
    "jest": "^23.1.0",
    "mini-application": "^1.0.6",
    "minihull": "^2.1.2",
    "moxios": "^0.4.0",
    "nock": "^9.3.0",
    "pret": "^0.0.1",
    "prettier": "^1.13.4",
    "react-hot-loader": "^4.2.0",
    "reactotron-react-js": "^1.12.2",
    "reactotron-redux": "^1.12.2"
  }
}
