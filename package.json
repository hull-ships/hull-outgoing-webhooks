{
  "name": "hull-webhooks",
  "description": "Send Hull user updates as webhooks",
  "version": "0.2.3",
  "homepage": "https://github.com/hull-ships/hull-webhooks",
  "license": "MIT",
  "main": "bin/start",
  "author": {
    "name": "Hull",
    "email": "contact@hull.io",
    "url": "https://github.com/hull"
  },
  "engines": {
    "node": "6.x",
    "npm": "3.x"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/hull-ships/hull-webhooks"
  },
  "bugs": {
    "url": "https://github.com/hull-ships/hull-webhooks/issues"
  },
  "scripts": {
    "ngrok": "ngrok http 8082 --region eu --subdomain webhooks",
    "build": "npm run clean && npm run build:server",
    "build:server": "./node_modules/.bin/babel server -d lib",
    "start": "node ./lib",
    "start:dev": "babel-watch -L server",
    "test": "npm run test:lint && npm run test:units && npm run test:flow",
    "test:lint": "eslint server",
    "test:units": "NODE_ENV=test mocha -R spec ./tests/index",
    "test:flow": "flow check",
    "clean": "./node_modules/.bin/rimraf dist; ./node_modules/.bin/rimraf lib",
    "update": "./node_modules/.bin/updtr",
    "watch": "./node_modules/.bin/watch 'npm run test:lint -s && npm run test:units -s' server tests",
    "postinstall": "npm run build"
  },
  "keywords": [
    "hull",
    "ship",
    "hull-ship",
    "webhooks",
    "integration",
    "user",
    "sync"
  ],
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-watch": "^2.0.6",
    "eslint": "^3.14.1",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0",
    "flow-bin": "^0.45.0",
    "updtr": "^0.2.3"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.22.0",
    "body-parser": "^1.16.0",
    "bson-objectid": "^1.1.5",
    "chai": "^3.5.0",
    "datadog-metrics": "^0.3.0",
    "dogapi": "^2.6.0",
    "ejs": "^2.5.5",
    "express": "^4.14.1",
    "hull": "^0.11.8",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.4",
    "minimist": "^1.2.0",
    "mocha": "^3.2.0",
    "moment": "^2.17.1",
    "moxios": "^0.4.0",
    "newrelic": "^1.36.2",
    "raven": "^1.1.1",
    "restler": "^3.4.0",
    "rimraf": "^2.5.4",
    "sinon": "^2.1.0",
    "winston-logstash": "^0.3.0"
  }
}
